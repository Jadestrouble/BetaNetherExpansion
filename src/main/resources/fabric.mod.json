{
  "schemaVersion": 1,
  "id": "netherexpansion",
  "version": "${version}",

  "name": "Nether Expansion",
  "description": "A mod that adds Fortresses into Beta 1.7.3.",
  "authors": [
    "Jadestrouble"
  ],
  "contact": {
    "homepage": "https://github.com/Jadestrouble/BetaNetherExpansion",
    "sources": "https://github.com/Jadestrouble/BetaNetherExpansion"
  },

  "license": "CC0-1.0",
  "icon": "assets/netherexpansion/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "jadestrouble.netherplus.blocks.NetherBrickBlocks",
      "jadestrouble.netherplus.blocks.NetherRecipes"
    ],
    "stationapi:event_bus_client": [
      "jadestrouble.netherplus.blocks.NetherBrickTextures"
    ],
    "stationapi:event_bus_server": [
    ]
  },
  "mixins": [
    "netherexpansion.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}
